@model IEnumerable<OrgProject.Models.Type>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<style>
    .au-btn {
        margin: 30px 0 30px 10px;
    }

        .au-btn a {
            color: #fff;
        }

            .au-btn a:hover {
                color: #000;
            }

    .block-email {
        line-height: 41px;
        font-size: 25px;
    }
</style>
<h2 style="margin-top:25px;"> Mangment Type </h2>

<button class="au-btn au-btn-icon au-btn--green au-btn--small">
    <i class="zmdi zmdi-plus"></i>   @Html.ActionLink("New Type", "Create")
</button>

<div class="row">
    <div class="col-md-12">


        <div class="table-responsive table-responsive-data2">
            <table class="table table-data2">
                <thead>
                    <tr>
                        <th>Name </th>
                        <th></th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr class="tr-shadow" id="row_@item.id">

                            <td>
                                <span class="block-email">
                        @Html.DisplayFor(x=>item.Type1)
                                </span>
                            </td>
                           <td>
                               
                            </td>
                        </tr>
                        <tr class="spacer"></tr>

                    }




                </tbody>
            </table>
        </div>
    </div>
</div>



<script src="~/Scripts/jquery-3.3.1.min.js"></script>
<script>

    function nnn() {
    var successMessage = '@TempData["sucessMsg"]'
    if (successMessage != '') {
        alertify.success(successMessage);
    }
    }

    $(document).ready(function () {
        nnn();

    });


</script>
