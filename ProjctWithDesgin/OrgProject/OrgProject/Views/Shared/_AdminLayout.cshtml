<body>
    <div class="page-wrapper">
        <!-- HEADER DESKTOP-->
        <header class="header-desktop3 d-none d-lg-block">
            <div class="section__content section__content--p35">
                <div class="header3-wrap">
                    <div class="header__logo">
                        <a href="@Url.Action("Index","Home")">
                            <img src="~/Content/MyStyle/MyImageStyle/logo.png" alt="Home" />
                        </a>
                    </div>
                    <div class="header__navbar">
                        <ul class="list-unstyled">


                            <li>
                                <a href="@Url.Action("Index", "AdminFinalProject")">
                                    <i class="fas fa-shopping-basket"></i>
                                    <span class="bot-line"></span>Final Project
                                </a>
                            </li>



                            <li>
                                <a href="@Url.Action("Index", "AdminFinalMaster")">
                                    <i class="fas fa-shopping-basket"></i>
                                    <span class="bot-line"></span> Final Master
                                </a>
                            </li>



                            <li class="has-sub">
                                <a href="#">
                                    <i class="fas fa-tachometer-alt"></i>info, User
                                    <span class="bot-line"></span>
                                </a>
                                <ul class="header3-sub-list list-unstyled">
                                    <li>
                                        <a href="@Url.Action("Index", "AdminManageUsers")"> Users  </a>
                                    </li>
                                    <li>
                                        <a href="@Url.Action("Index", "AdminManageComments")"> Comments </a>
                                    </li>
                                    <li>
                                        <a href="@Url.Action("Index", "AdminManageType")"> Type </a>
                                    </li>

                                </ul>
                            </li>




                            <li class="has-sub">
                                <a href="#">
                                    <i class="fas fa-copy"></i>
                                    <span class="bot-line"></span>More_Info
                                </a>
                                <ul class="header3-sub-list list-unstyled">
                                    <li>
                                        <a href="@Url.Action("Index", "AdminManageQuestions")"> Questions </a>
                                    </li>
                                    <li>
                                        <a href="@Url.Action("Index", "AdminGuide")">Guide</a>
                                    </li>
                                    <li>
                                        <a href="@Url.Action("Index", "AdminCourceEachYear")"> Course Each Year </a>
                                    </li>
                                </ul>
                            </li>





                            <li>
                                <a href="@Url.Action("Index", "AdminDepartment")">
                                    <i class="fas fa-trophy"></i>
                                    <span class="bot-line"></span> Department
                                </a>
                            </li>



                            <li>
                                <a href="@Url.Action("Index", "ChatRoom")">
                                    <i class="fas fa-trophy"></i>
                                    <span class="bot-line"></span>Chat Room
                                </a>
                            </li>



                        </ul>
                    </div>
                    <div class="header__tool">
                        <div class="account-wrap">
                            <div class="account-item account-item--style2 clearfix js-item-menu">
                                <div class="image">
                                    <img src="~/PdfImageVideo/images/NoPic.jpg" alt="" />
                                </div>

                                <div class="content">
                                    <a class="js-acc-btn" href="#"> @NewEmail </a>
                                </div>
                                <div class="account-dropdown js-dropdown">
                                    <div class="info clearfix">
                                        <div class="image">
                                            <a href="#">
                                                <img src="~/PdfImageVideo/images/NoPic.jpg" alt=" @User.Identity.Name " />
                                            </a>
                                        </div>
                                        <div class="content">
                                            <h5 class="name">
                                                @NewEmail
                                            </h5>

                                            <span class="email"> @User.Identity.Name  </span>
                                        </div>
                                    </div>
                                    <div class="account-dropdown__body">
                                        <div class="account-dropdown__item">
                                            @*<a href="#">*@
                                            <a href="@Url.Action("ViewProfile","Profile", new { Email = User.Identity.Name } )">
                                                <i class="zmdi zmdi-account"></i> Account
                                            </a>
                                        </div>


                                    </div>
                                    <div class="account-dropdown__footer">
                                        @*<a href="#">*@
                                        <a href="@Url.Action("logOut","Account")">
                                            <i class="zmdi zmdi-power"></i>Logout
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </header>
        <!-- END HEADER DESKTOP-->
        <!-- HEADER MOBILE-->
        <header class="header-mobile header-mobile-2 d-block d-lg-none">
            <div class="header-mobile__bar">
                <div class="container-fluid">
                    <div class="header-mobile-inner">
                        <a class="logo" href="index.html">
                            <img src="images/icon/logo-white.png" alt="CoolAdmin" />
                        </a>
                        <button class="hamburger hamburger--slider" type="button">
                            <span class="hamburger-box">
                                <span class="hamburger-inner"></span>
                            </span>
                        </button>
                    </div>
                </div>
            </div>
            <nav class="navbar-mobile">
                <div class="container-fluid">
                    <ul class="navbar-mobile__list list-unstyled">


                        <li>
                            <a href="@Url.Action("Index", "AdminFinalProject")">
                                <i class="fas fa-chart-bar"></i>Final Project
                            </a>
                        </li>

                        <li>
                            <a href="@Url.Action("Index", "AdminFinalMaster")">
                                <i class="fas fa-shopping-basket"></i>Final Master
                            </a>
                        </li>


                        <li class="has-sub">
                            <a class="js-arrow" href="#">
                                <i class="fas fa-copy"></i> info, User
                            </a>
                            <ul class="navbar-mobile-sub__list list-unstyled js-sub-list">
                                <li>
                                    <a href="@Url.Action("Index", "AdminManageUsers")"> Users  </a>
                                </li>
                                <li>
                                    <a href="@Url.Action("Index", "AdminManageComments")"> Comments </a>
                                </li>
                                <li>
                                    <a href="@Url.Action("Index", "AdminManageType")"> Type </a>
                                </li>
                            </ul>
                        </li>

                        <li class="has-sub">
                            <a class="js-arrow" href="#">
                                <i class="fas fa-copy"></i> More_Info
                            </a>
                            <ul class="navbar-mobile-sub__list list-unstyled js-sub-list">
                                <li>
                                    <a href="@Url.Action("Index", "AdminManageQuestions")"> Questions </a>
                                </li>
                                <li>
                                    <a href="@Url.Action("Index", "AdminGuide")">Guide</a>
                                </li>
                                <li>
                                    <a href="@Url.Action("Index", "AdminCourceEachYear")"> Course Each Year </a>
                                </li>
                            </ul>
                        </li>



                        <li>
                            <a href="@Url.Action("Index", "AdminDepartment")">
                                <i class="fas fa-trophy"></i> Department
                            </a>
                        </li>

                        <li>
                            <a href="@Url.Action("Index", "ChatRoom")">
                                <i class="fas fa-trophy"></i> Chat
                            </a>
                        </li>


                    </ul>
                </div>
            </nav>
        </header>
        <div class="sub-header-mobile-2 d-block d-lg-none">
            <div class="header__tool">


                <div class="account-wrap">
                    <div class="account-item account-item--style2 clearfix js-item-menu">
                        <div class="image">
                            <img src="~/PdfImageVideo/images/NoPic.jpg" alt=" @User.Identity.Name " />
                        </div>
                        <div class="content">
                            <a class="js-acc-btn" href="#"> @NewEmail </a>
                        </div>
                        <div class="account-dropdown js-dropdown">
                            <div class="info clearfix">
                                <div class="image">
                                    <a href="#">
                                        <img src="~/PdfImageVideo/images/NoPic.jpg" alt=" @User.Identity.Name " />
                                    </a>
                                </div>
                                <div class="content">
                                    <h5 class="name">
                                        @NewEmail
                                    </h5>
                                    <span class="email"> @User.Identity.Name </span>
                                </div>
                            </div>
                            <div class="account-dropdown__body">
                                <div class="account-dropdown__item">
                                    <a href="@Url.Action("ViewProfile","Profile", new { Email = User.Identity.Name } )">
                                        <i class="zmdi zmdi-account"></i>Account
                                    </a>
                                </div>

                            </div>
                            <div class="account-dropdown__footer">
                                <a href="@Url.Action("logOut","Account")">
                                    <i class="zmdi zmdi-power"></i>Logout
                                </a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- END HEADER MOBILE -->
        <!-- PAGE CONTENT-->
        <div class="page-content--bgf7">


            <!--  My Content -->
            <section class="p-t-20">
                <div class="container">
                    @RenderBody()
                </div>
            </section>

            <!-- END DATA TABLE-->
            <!-- COPYRIGHT-->
            <section class="p-t-60 p-b-20">
                <div class="container">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="copyright">
                                <p>Copyright © 2018 Turboo. All rights reserved. Template by <a href="https://www.linkedin.com/in/eslam-akrm-58aa53138/" target="_blank">Eslam</a>.</p>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
            <!-- END COPYRIGHT-->
        </div>

    </div>

    @*<script src="vendor/jquery-3.2.1.min.js"></script>*@
    <script src="~/Scripts/jquery-3.3.1.min.js"></script>


    @*<script src="vendor/bootstrap-4.1/bootstrap.min.js"></script>*@
